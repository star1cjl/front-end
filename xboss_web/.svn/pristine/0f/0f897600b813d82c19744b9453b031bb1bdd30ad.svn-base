    <script type="text/javascript">
    util.initPage();
    $(document).ready(function() {
        $('#container').on(EV, ".toFilter", function() {
            //$("body").css("overflow", "hidden");
            $(window).off("scroll");
            util.showActionSheet(function() {
                $(window).on("scroll", function() {
                    fnScroll();
                });

                var consumptionType = $("#consumptionTypeWrap p.active").map(function() {
                    return $(this).text();
                }).get().join(",");

                var settleType = $("#settleTypeWrap p.active").map(function() {
                    return $(this).text();
                }).get().join(",");

                var storeNames = $("#storeWrap p.active").map(function() {
                    return $(this).text();
                }).get().join(",");

                var filterRange = $('#filterRangeDate').mobiscroll("getValue");
                console.log(filterRange[0].format());
                console.log(filterRange[1].format());

                //$("body").css("overflow", "auto");
            });
        });

        $("#container").on(EV, ".weui_grids", function() {
            $(this).css("background-color", "#e4e4e4");
            window.location.href = "detail.html?dType=trade";
        });

        function isOnTop(index, obj) {
            var headerH = $(".x_header").height(),
                scrollTop = $("body").scrollTop(),
                offsetTop = $(obj).offset().top,
                objH = $(obj).height();
            index == 0 && (headerH = 0);
            return headerH + scrollTop > offsetTop && headerH + scrollTop < offsetTop + objH;
        }

        var dateFieldName = "F_L_TRADE_TIME_MD",
            pageIndex = 1,
            scrollTimer;

        function query(params) {
            $.ajax({
                url: "",
                type: "POST",
                data: params,
                dataType: "json",
                beforeSend: function() {
                    params.pageIndex == 1 && util.loadingToast();
                },
                success: function(data) {
                    util.removeToast();
                    if (data && (data.code == 200 || data.rtnCode == 200)) {
                        renderData(data.data);
                        data.totalPage == 1 || params.pageIndex == data.totalPage ? $("#pullUp").addClass("dn") : $("#pullUp").removeClass("dn");
                    } else {
                        util.dialog({
                            info: data.msg || data.rtnMsg
                        });
                    }
                },
                error: function() {
                    util.dialog({
                        info: "服务器异常，请稍候重试！"
                    });
                }
            });
        }

        function renderData(data) {
            var groupKey,
                groupObj = {};
            data.forEach(function(obj) {
                var dateArr = obj[dateFieldName].split("-");
                groupKey = dateArr[0] + dateArr[1];
                groupObj[groupKey] = groupObj[groupKey] || [];
                obj[dateFieldName] = dateArr[1] + dateArr[2];
                groupObj[groupKey].push(obj);
            });

            var tpl = $("#tpl_transaction_flw").html();
            for (groupKey in groupObj) {
                var html = "";
                groupObj[groupKey].forEach(function(obj) {
                    html += tpl.format(obj);
                });
                if (!$("#flw" + groupKey).length) {
                    var date = new Date(),
                        month = date.getMonth() + 1;
                    var tMonth = parseInt(groupKey.substring(4), 10);
                    var monthLabel = tMonth == month ? "本月" : tMonth + "月";

                    var outerHtml = $("#tpl_month_flw").html(),
                        outerHtml = $(outerHtml);
                    outerHtml.attr("id", "#flw" + groupKey)
                        .find(".month_label").html(monthLabel);
                    outerHtml.find(".data_wrap").append(html);
                    $("#container").append(outerHtml);
                } else {
                    $("#flw" + groupKey).find(".data_wrap").append(html);
                }
            }
        }

        function fnScroll() {
            clearTimeout(scrollTimer);
            scrollTimer = setTimeout(function() {
                jQuery.each($(".month_flw_wrap"), function(index) {
                    if (isOnTop(index, this)) {
                        $(".x_header", this).addClass("nav_fixed");
                        $(".toFilter", this).removeClass("dn");
                    } else {
                        $(".x_header", this).removeClass("nav_fixed");
                        $(".toFilter", this).addClass("dn");
                    }
                });

                if (!$("#pullUp").hasClass("dn") && $("#pullUp").offset().top <= $("body").scrollTop() + $(window).height()) {
                    query(++pageIndex);
                }
            }, 200);
        }

        $(window).on("scroll", function() {
            fnScroll();
        });

    });
    </script>
</body>

